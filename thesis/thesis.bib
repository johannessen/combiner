file = {{2011 Klammer.pdf:/Users/aj3/Studium/DA/Ref/[Kla11] Integration von Generalisierungsfunktionalität für die automatischen Ableitung verschiedener Levels of Detail von OpenstreetMap Webkarten.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/E68E697C-036C-463A-A95D-9EE33E1BD0B0}}
}

