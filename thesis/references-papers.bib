@phdthesis{Kla11,
author = {Klammer, Ralf},
title = {{Integration von Generalisierungsfunktionalit{\"a}t f{\"u}r die automatischen Ableitung verschiedener Levels of Detail von OpenstreetMap Webkarten}},
school = {Technische Universit{\"a}t Dresden},
year = {2011},
address = {Dresden},
publisher = {Technische Universit{\"a}t Dresden},
keywords = {DA},
language = {German},
